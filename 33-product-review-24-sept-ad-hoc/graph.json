    "decisions": [
      {
        "id": "decision_entity_attribute_value_pattern_for_assets",
        "type": "Decision",
        "description": "Implement Entity Attribute Value pattern for asset management instead of fixed columns approach used in vehicles table",
        "decided_by": "Dan Khasis",
        "rationale": "CEO-level architectural decision for maximum flexibility. EAV pattern allows unlimited attributes per asset without schema modifications, eliminating constant code base changes for elastic indexing. Enables support for diverse asset types across new market verticals without database restructuring. [CEO_APPROVED]",
        "alternatives": [
          "Continue with fixed columns in vehicles table",
          "Create separate tables for each asset type"
        ],
        "topic_id": "topic_asset_management_schema_architecture_design"
      },
      {
        "id": "decision_asset_categorization_hierarchy",
        "type": "Decision",
        "description": "Implement three-level hierarchy: asset master list, asset types with categories, and user-defined groups",
        "decided_by": "Dan Khasis",
        "rationale": "CEO dictating data model architecture. Three-level approach provides both system-level classification and user flexibility. System maintains type taxonomy while allowing arbitrary user grouping for operational needs. [CEO_APPROVED]",
        "alternatives": [
          "Single-level flat structure",
          "Two-level type/instance only"
        ],
        "topic_id": "topic_asset_management_schema_architecture_design"
      },
      {
        "id": "decision_attribute_type_categories",
        "type": "Decision",
        "description": "Create attribute type categories table for dynamic frontend grouping. Initially fixed categories, future customer-defined categories",
        "decided_by": "Dan Khasis, Igor Skrynkovskyy",
        "rationale": "Enables dynamic frontend rendering without hardcoded columns. Dan approved phased approach - fixed categories in version 1, customer categories as future enhancement. [CEO_APPROVED]",
        "alternatives": [
          "UUID-based extensible system from start",
          "Hardcoded attribute groups"
        ],
        "topic_id": "topic_asset_management_schema_architecture_design"
      },
      {
        "id": "decision_physical_and_logical_group_support",
        "type": "Decision",
        "description": "Support both physical and logical group classifications simultaneously",
        "decided_by": "Dan Khasis",
        "rationale": "CEO's gut instinct that assets can be in both physical location grouping and logical operational grouping simultaneously. Hospital equipment example - physically in surgery department, logically on mobile cart. [CEO_APPROVED]",
        "alternatives": [],
        "topic_id": "topic_asset_management_schema_architecture_design"
      },
      {
        "id": "decision_strategic_planner_snapshot_ui_design",
        "type": "Decision",
        "description": "Read-only snapshot must copy Route List UI/UX exactly - table view, interactive map in separate tab, full filtering capabilities",
        "decided_by": "Dan Khasis",
        "rationale": "CEO dictating critical sales blocker resolution. Current snapshot is uninformative and prevents demos. Copying proven Route List design ensures usability and leverages existing user familiarity. [CRITICAL_DIRECTIVE]",
        "alternatives": [
          "Custom snapshot-specific UI",
          "Simplified report view"
        ],
        "topic_id": "topic_strategic_planner_snapshot_ui_requirements"
      },
      {
        "id": "decision_day_and_week_filtering",
        "type": "Decision",
        "description": "Add filters by day and week that activate when user clicks matrix results cells",
        "decided_by": "Dan Khasis",
        "rationale": "CEO identifying fundamental missing requirement. Users must click matrix cell and immediately see filtered routes for that day/week on map and table. This is baseline functionality for scenario analysis. [CRITICAL_DIRECTIVE]",
        "alternatives": [],
        "topic_id": "topic_strategic_planner_snapshot_ui_requirements"
      },
      {
        "id": "decision_route_comparison_feature",
        "type": "Decision",
        "description": "Implement multi-route selection and side-by-side comparison on single map, similar to e-commerce product comparison",
        "decided_by": "Dan Khasis",
        "rationale": "CEO demanding competitive feature. Clients don't believe optimization results without visual proof. Need ability to select multiple instances of same route across different time periods and visually compare side-by-side. Legacy route comparison exists but needs modernization. [CEO_APPROVED]",
        "alternatives": [],
        "topic_id": "topic_strategic_planner_snapshot_ui_requirements"
      },
      {
        "id": "decision_restore_ios_foreground_service",
        "type": "Decision",
        "description": "Immediately restore Foreground Service functionality in iOS app to enable background GPS tracking",
        "decided_by": "Dan Khasis",
        "rationale": "CEO directive to fix critical data quality issue. Foreground Service was previously disabled but is essential for continuous tracking. Current sparse data from Amwaste proves background tracking failure. Without this, product analytics are broken. [CRITICAL_DIRECTIVE]",
        "alternatives": [],
        "topic_id": "topic_gps_data_quality_issues_and_bigquery_integration"
      },
      {
        "id": "decision_mobile_session_analytics",
        "type": "Decision",
        "description": "Implement collection and analysis of mobile app active session duration metrics",
        "decided_by": "Dan Khasis",
        "rationale": "CEO ordering proof mechanism. Need data to demonstrate to clients that problem is driver behavior closing app, not system malfunction. Session metrics provide objective evidence. [CEO_APPROVED]",
        "alternatives": [],
        "topic_id": "topic_gps_data_quality_issues_and_bigquery_integration"
      },
      {
        "id": "decision_bigquery_gps_data_integration",
        "type": "Decision",
        "description": "Prioritize and implement export of all GPS data to BigQuery",
        "decided_by": "Dan Khasis",
        "rationale": "CEO strategic directive for analytics infrastructure. BigQuery integration unlocks advanced analytics capabilities currently impossible. This is foundational for future data science initiatives. [CRITICAL_DIRECTIVE]",
        "alternatives": [],
        "topic_id": "topic_gps_data_quality_issues_and_bigquery_integration"
      },
      {
        "id": "decision_flexible_time_window_validation",
        "type": "Decision",
        "description": "Modify optimization and validation logic to support service time exceeding time window duration, validating only arrival time",
        "decided_by": "Dan Khasis",
        "rationale": "CEO identifying unique competitive advantage from client discussion. Real-world scenarios require arrival during restricted access window but service extending beyond. Current competitor systems fail this use case. Implementing this unlocks heavy logistics, construction, and service maintenance verticals. [CEO_APPROVED]",
        "alternatives": [
          "Maintain current strict time window validation",
          "Split long services into multiple stops"
        ],
        "topic_id": "topic_flexible_service_time_and_time_window_logic"
      },
      {
        "id": "issue_route_comparison_ui_broken",
        "type": "TechnicalIssue",
        "description": "**[01:15:30] Serhii Kasainov**: Route comparison interface broken. Something broke during today's deployment. List that should have 25 elements not appearing. No output, nothing else.\n\n**[01:16:45] Dan Khasis**: Console shows possible API call failure. Grouping by facility type also missing. Multiple UI elements not rendering properly.",
        "decided_by": "Team",
        "rationale": "Technical constraint identified during discussion",
        "alternatives": [],
        "topic_id": "topic_route_comparison_legacy_system_issues"
      }
